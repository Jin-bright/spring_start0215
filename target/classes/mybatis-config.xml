<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
	  <setting name="jdbcTypeForNull" value="NULL"/>
	  <setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<!-- resulttype 너무 기니까 별칭등록하기 -->
	<typeAliases>
		<package name="com.sh.spring"  /> <!-- 지정한 패키지 하위의 모든 클래스를 소문자로 시작하는 별칭으로 등록  -->
	</typeAliases>
	
	<typeHandlers>
	<!--	<typeHandler handler="com.sh.spring.common.typehandler.StringArrayTypeHandler"/>-->
		<package name="com.sh.spring.common.typehandler"/>
	</typeHandlers>

 
</configuration>